<%- include("../required/header") %>
<style>
    .search-box{
        margin:-1.4rem -.8rem 0 2.3rem;
    }
    @media (max-width:768px){
        .search-box{
            margin:-1.4rem -2.5rem 0 2.3rem;
        }
    }
</style>
<main>
    <main id="block14">
      <div class="row" >
        <div class="col-lg-12 col-md-12 col-sm-12">
          <div class="bx-shadow ">
            <p class="heading-search font-weight-bold text-left pl-2">Room change request</p>
          </div>
        </div>
        <div class="col-lg-12 col-md-12 col-sm-12">
          <div class="search-box" >
            <br>
  
            <div class="col-lg-12 col-md-12 col-sm-12 search-bx search-bx2 text-center cursor-pointer">
              <div class="table-responsive "  style="overflow-x:auto; ">
                <table class="table p-2 mt-3 text-center" style="margin:0 auto;">
                    <thead>
                        <tr>
                            <th scope="col">SINo</th>
                            <th scope="col">Name</th>
                            <th scope="col">couse & year</th>
                            <th scope="col">From</th>
                            <th scope="col">To</th>
                            <th scope="col">Action</th>
                            <th scope="col">Reason</th>
                            <th scope="col">Date</th>
                        </tr>
                    </thead>
                    <tbody>
                      <% if(request) { %>
                        <% for (var i=0; i < request.length; i++) { %>
                          <% if(request[i].clicked) { %>
                            
                             
                        <tr>
                            <td>1</td>
                            <td><%= request[i].student_name %></td>
                            <td><%= request[i].student_course %></td>    

                            <% for (var j=0; j < room.length; j++) { %>
                              <% if(JSON.stringify(room[j]._id) == JSON.stringify(request[i].room)) { %>
                                <td><%= room[j].room_name %></td>
                            <% } %>
                          <% } %>

                          <% for (var j=0; j < room.length; j++) { %>
                            <% if(JSON.stringify(room[j]._id) == JSON.stringify(request[i].targetroom)) { %>
                            <td><%= room[j].room_name %></td>
                            <% } %>
                            <% } %>

                            <td><%= request[i].status %></td>

                            <td><%= request[i].remarks %></td>
                            <td><%= request[i].Requested %></td>
                        </tr>
                        
                        <% } %>
                        <% } %>
                        <% } %>
                        
                        
              
                    </tbody>

                </table>
                <br>

            </div>
                
              
            </div>
          </div>
        </div>
      </div>
    </main>
  </main>